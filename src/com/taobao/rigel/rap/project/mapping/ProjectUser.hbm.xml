<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.taobao.rigel.rap.project.bo.ProjectUser" table="tb_project_and_user">
		<composite-id>
			<key-property name="userId">
				<column name="user_id" />
			</key-property>
			<key-property name="projectId">
				<column name="project_id" />
			</key-property> 
       </composite-id> 
		<property name="accesslevel">
			<column name="access_level" />
		</property>        
	</class>
</hibernate-mapping>